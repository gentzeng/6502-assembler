{
  "name": "6502assembler",
  "version": "1.0.0",
  "description": "A JavaScript 6502 assembler and simulator ",
  "main": "assembler.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "test": "jest",
    "fmt-js": "npx prettier -w ./assets/js/*",
    "fmt-html": "npx prettier -w ./index.html",
    "fmt": "npm run fmt-html; npm run fmt-js",
    "link": "node_modules/.bin/rollup -c",
    "compile": "npm run link"
  },
  "keywords": [
    "6502",
    "simulator",
    "assembler"
  ],
  "author": "Georg Gentzen, Stian Soreng",
  "license": "GPL-1.0",
  "dependencies": {
    "@codemirror/basic-setup": "^0.19.0",
    "@codemirror/lang-javascript": "^0.19.2",
    "@codemirror/legacy-modes": "^0.19.0",
    "@codemirror/stream-parser": "^0.19.3",
    "@popperjs/core": "^2.11.5",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "bootstrap": "^5.1.3",
    "http-server": "^14.0.0",
    "jquery": "^3.6.0",
    "rollup": "^2.58.1"
  },
  "devDependencies": {
    "prettier": "2.5.1",
    "rollup-plugin-external-globals": "^0.6.1"
  }
}
